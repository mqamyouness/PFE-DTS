<section class="ftco-counter ftco-bg-dark img" id="section-counter" style="background-image: url(/Content/images/bg_2.jpg);" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-10">
                <div class="row">
                    <div class="col-md-6 col-lg-3 d-flex justify-content-center counter-wrap ftco-animate">
                        <div class="block-18 text-center">
                            <div class="text">
                                <strong class="number" id="react1" data-number="0">0</strong>
                                <span>Number Users</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-3 d-flex justify-content-center counter-wrap ftco-animate">
                        <div class="block-18 text-center">
                            <div class="text">
                                <strong class="number" id="react2" data-number="0">0</strong>
                                <span>Number Coaches</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-3 d-flex justify-content-center counter-wrap ftco-animate">
                        <div class="block-18 text-center">
                            <div class="text">
                                <strong class="number" id="react3" data-number="0">0</strong>
                                <span>Number Posts</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-3 d-flex justify-content-center counter-wrap ftco-animate">
                        <div class="block-18 text-center">
                            <div class="text">
                                <strong class="number" id="react4" data-number="0">0</strong>
                                <span>Number Client</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<script>
    document.getElementById("react1").setAttribute("data-number",@ViewBag.Numbers[0]);
    document.getElementById("react2").setAttribute("data-number",@ViewBag.Numbers[1]);
    document.getElementById("react3").setAttribute("data-number",@ViewBag.Numbers[2]);
    document.getElementById("react4").setAttribute("data-number",@ViewBag.Numbers[3]);
</script>
